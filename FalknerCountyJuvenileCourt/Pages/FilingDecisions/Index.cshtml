@page
@model FalknerCountyJuvenileCourt.Pages.FilingDecisions.IndexModel

@{
    ViewData["Title"] = "Filing Decisions";
}



<h1>Filing Decisions</h1>

<p>
    <a id="Cbtn" asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FilingDecisions)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.FilingDecisions) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a id="EDbtn" asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a id="EDbtn" asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
